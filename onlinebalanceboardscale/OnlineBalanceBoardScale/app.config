 <?xml version="1.0" encoding="utf-8" ?>

<configuration>
  <system.net>
    <defaultProxy enabled="true" useDefaultCredentials="false">
      <module type="OnlineBalanceBoardScale.proxy, OnlineBalanceBoardScale" />
    </defaultProxy>
  </system.net>


  <appSettings>
    <add key="ProxyAddress" value="http://proxyarg:80/" />
    <add key="ProxyUser" value="" />
    <add key="ProxyPass" value="" />
    <add key="ProxyDomain" value="" />
  </appSettings>

</configuration>

